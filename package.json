{
  "name": "my-ecommerce",
  "version": "1.0.0",
  "description": "shopping cart",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "node server.js --prefix frontend",
    "dev": "concurrently \"npm run server \" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eo640217/meal-ecommerce.git"
  },
  "author": "Emmanuel Olufelo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eo640217/meal-ecommerce/issues"
  },
  "homepage": "https://github.com/eo640217/meal-ecommerce#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14"
  }
}
